<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <config>
    <add key="repositoryPath" value="packages" />
  </config>
  <packageSources>
    <clear />
    <add key="nuget" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="KSociety.Base" value="https://ci.appveyor.com/nuget/ksociety.base" />
	<add key="KSociety.Log" value="https://ci.appveyor.com/nuget/ksociety.log" />
  </packageSources>

  <packageSourceMapping>
    <packageSource key="nuget">
      <package pattern="*" />
    </packageSource>
    <packageSource key="KSociety.Base">
      <package pattern="KSociety.Base.*" />
    </packageSource>
	<packageSource key="KSociety.Log">
      <package pattern="KSociety.Log.*" />
    </packageSource>
  </packageSourceMapping>
</configuration>